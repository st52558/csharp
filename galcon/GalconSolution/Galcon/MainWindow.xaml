<Window x:Class="Galcon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Galcon"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" ResizeMode="NoResize" Background="White">
    <Grid>
        <Canvas x:Name="canvas" Uid="canvas" Loaded="Canvas_Loaded" Margin="0,0,0,0" Width="800" Height="450" MouseWheel="Canvas_MouseWheel" Background="White"/>
    </Grid>
</Window>
